{
  "profiles": {
    "GloryBot Start": {
      "commandName": "Executable",
      "executablePath": "electronize",
      "commandLineArgs": "start",
      "workingDirectory": ".",
      "nativeDebugging": false,
      "jsWebView2Debugging": false
    },
    "GloryBot Watch": {
      "commandName": "Executable",
      "executablePath": "electronize",
      "commandLineArgs": "start /watch",
      "workingDirectory": "."
    },
    "GloryBot Build Win": {
      "commandName": "Executable",
      "executablePath": "electronize",
      "commandLineArgs": "build /target win",
      "workingDirectory": "."
    },
    "GloryBot Deploy": {
      "commandName": "Executable",
      "executablePath": "electronize",
      "commandLineArgs": "build /target win /electron-params \"-p always\"",
      "workingDirectory": "."
    },
    
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  },
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:37927",
      "sslPort": 44330
    }
  }
}